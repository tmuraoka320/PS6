library(devtools); library(roxygen2)
createCandidate <- function(name, delegatesWon, party){
setClass(Class="Candidate",
slots=list(name="character",
delegatesWon="numeric",
party="character",
delegateNeeded="numeric"))
setValidity("Candidate", function(object){
if(is.character(object@name)==FALSE){
print("name should be character!")
}
if(is.numeric(object@delegatesWon)==FALSE){
print("delegatesWon should be numeric!")
}
if(object@party!="Democratic" | object@party!="Republican"){
print("party should be Democratic or Republican!")
}
})
if(party!="Democratic" | party!="Republican"){
print("party should be Democratic or Republican!")
}
else{
delNeed <- ifelse(party=="Democratic", 2383, 1237)
}
object <- new("Candidate",
name=name,
delegatesWon=delegatesWon,
party=party,
delegatesNeeded=delNeed)
return(object)
}
createCandidate("obama", 44, "Democratic")
